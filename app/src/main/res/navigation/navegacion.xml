<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegacion"
    app:startDestination="@id/inicioFragment">

    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.jzamudio.isla21410.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicioFragment_to_playasFragment"
            app:destination="@id/playasFragment" />
    </fragment>
    <fragment
        android:id="@+id/playasFragment"
        android:name="com.jzamudio.isla21410.Playas.playasFragment"
        android:label="playas_fragment"
        tools:layout="@layout/playas_fragment" >
        <action
            android:id="@+id/action_playasFragment_to_playaCentralFragment"
            app:destination="@id/playaCentralFragment" />
        <action
            android:id="@+id/action_playasFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
    </fragment>
    <fragment
        android:id="@+id/playaCentralFragment"
        android:name="com.jzamudio.isla21410.Playas.listPlayas.Central.playaCentralFragment"
        android:label="playa_central_fragment"
        tools:layout="@layout/playa_central_fragment" />
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.jzamudio.isla21410.Playas.listPlayas.Central.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
</navigation>